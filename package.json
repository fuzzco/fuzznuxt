{
    "name": "fuzznuxt",
    "version": "5.0.2",
    "description": "Fuzzco Nuxt boilerplate.",
    "author": "Fuzzco",
    "bin": {
        "fuzznuxt": "./scripts/scaffold/npx.sh"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
        "build": "nuxt build",
        "start": "nuxt start",
        "generate": "nuxt generate",
        "test": "npm run generate; http-server dist & FUZZNUXT_SRV_PID=$!; node test; kill -9 $FUZZNUXT_SRV_PID",
        "heroku-postbuild": "npm run build",
        "fonts": "sh bash/fonts.sh",
        "deploy": "git push heroku master"
    },
    "prettier": {
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "engines": {
        "node": "9.11.2"
    },
    "dependencies": {
        "@babel/core": "7.6.4",
        "@fuzzco/font-loader": "^1.0.2",
        "@fuzzco/fragment-shader": "^1.1.0",
        "@fuzzco/in-view": "^1.2.2",
        "@fuzzco/scrub-wrap": "^1.1.1",
        "@fuzzco/super-image": "^1.1.1",
        "@fuzzco/vue-marquee": "^1.0.1",
        "@nuxtjs/component-cache": "1.1.5",
        "@nuxtjs/device": "^1.2.4",
        "@nuxtjs/prismic": "1.2.1",
        "@nuxtjs/style-resources": "^0.1.2",
        "@popmotion/popcorn": "^0.4.0",
        "auto-blur": "^1.1.2",
        "cross-fetch": "^3.0.2",
        "current-device": "^0.8.0",
        "express": "^4.16.4",
        "fh-components": "^1.4.15",
        "fitvids": "^2.0.0",
        "gittar": "0.1.1",
        "imagesloaded": "4.1.4",
        "js-cookie": "^2.2.0",
        "lodash": "4.17.19",
        "nuxt": "2.13.3",
        "phenomenon-px": "^1.0.0",
        "popmotion": "^8.6.4",
        "prismic-dom": "^2.1.0",
        "prismic-javascript": "^2.0.2",
        "prismic-vue": "2.0.1",
        "qs": "^6.7.0",
        "redirect-ssl": "^1.4.0",
        "screenfull": "^4.2.0",
        "v-runtime-template": "1.10.0",
        "vue": "2.6.11"
    },
    "devDependencies": {
        "@vue/test-utils": "^1.0.0-beta.27",
        "babel-core": "7.0.0-bridge.0",
        "child_process": "^1.0.2",
        "colors": "^1.3.3",
        "cross-env": "^5.2.0",
        "dotenv": "^7.0.0",
        "node-sass": "4.14.1",
        "nodemon": "2.0.4",
        "prettier": "^1.16.4",
        "prompts": "^2.1.0",
        "sass-loader": "^7.1.0"
    }
}
